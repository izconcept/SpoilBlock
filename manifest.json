{
  "manifest_version": 2,
  "name": "SpoilBlock",
  "version": "1.0",
  "description": "Automatically censors potential TV or movie spoilers on a web page",
  "browser_action": {
  "default_icon": "icon.png"
},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.2.3.min.js", "content.js"]
    }
  ],
  "background": {
  "scripts": ["background.js"]
}
}